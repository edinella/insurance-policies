{
  "name": "insurance-policies",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "./bin/seed; ./bin/www",
    "seed": "./bin/seed",
    "pretest": "npm install",
    "test": "export $(xargs < .env); NODE_ENV=development; ./node_modules/.bin/jest",
    "test:watch": "export $(xargs < .env); NODE_ENV=development; MONGO_HOST=localhost; MONGO_DATABASE=test; ./node_modules/.bin/jest --watch --runInBand --detectOpenHandles"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "argon2-ffi": "^1.1.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.1",
    "morgan": "^1.9.1",
    "pug": "^2.0.4",
    "require-directory": "^2.1.1",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
