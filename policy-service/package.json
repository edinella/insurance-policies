{
  "name": "insurance-policies-policy-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "./bin/ensure-initial-data ./bin/grpc",
    "pretest": "npm install",
    "test": "export $(xargs < ../.env); NODE_ENV=development; ./node_modules/.bin/jest",
    "test:watch": "export $(xargs < ../.env); NODE_ENV=development; ./node_modules/.bin/jest --watch"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "debug": "^4.1.1",
    "grpc": "^1.24.2",
    "mongoose": "^5.9.1",
    "require-directory": "^2.1.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1"
  }
}
